<script>
    function GetLatestUpdates() {

        if (ValidateEmail()) {
            var person = {
                email: $("#Email").val(),
            }

            $('#target').html('sending..');

            $.ajax({
                url: '/GoldenLife/GetLatestUpdates',
                type: 'post',
                dataType: 'json',
                success: function (data) {
                    $('#target').html(data.msg);
                    $("#Email").val("");
                },
                data: person
            });
        }
        else {
            return false;
        }
    }
    function ValidateEmail() {
        //Email validation
        var Email = $("#Email").val();
        if (Email !== '') {
            var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
            if (Email.length > 30) {
                alert('email length cannot exceed more than 30 characters.');
                $("#Email").addClass('validation_control');
                return false;
            } else if (!emailReg.test(Email)) {
                alert('please enter the valid email.');
                $("#Email").addClass('validation_control');
                return false;
            }
            else {
                $("#Email").removeClass('validation_control');
                return true;
            }
        }
        else {
            $("#Email").addClass('validation_control');
            alert('must enter emails.');
            return false;
        }

        //End
    }
</script>